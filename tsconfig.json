{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./types",
    "outDir": "dist",
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "strict": false,
    // "alwaysStrict": false,
    "strictNullChecks": false,
    // "useDefineForClassFields": true,
    // "sourceMap": true,
    "noUnusedLocals": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
      "vue/ref-macros"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "bin"
  ]
}
